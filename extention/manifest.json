{
    "name":"Phishing Detector",
    "description":"Phishing Detector",
    "version": "0.1.0",
    "manifest_version" : 2,
    "icons" :{
        "16":"./icon1.png",
        "32":"./icon1.png",
        "64":"./icon1.png",
        "128":"./icon1.png"
    },
    "background":{
        "scripts":["./background.js"]
    },
    "options_page":"./options.html",
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions":[
        "tabs",
        "https://www.google.com/*",
        "https://*/",
        "http://*/"
    ]

}